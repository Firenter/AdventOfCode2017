<Window x:Class="AoC2017WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AoC2017WPF"
        mc:Ignorable="d"
        Title="Advent of Code 2017" Height="720" Width="1280">
    <StackPanel Orientation="Horizontal" x:Name="MainPanel" Margin="0,0,0,0">
        <StackPanel Orientation="Vertical" Name="SimplePanel"/>
        <StackPanel Orientation="Vertical" Name="AdvancedPanel"/>
    </StackPanel>
</Window>
